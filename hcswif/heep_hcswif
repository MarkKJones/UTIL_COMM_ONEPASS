
python3 hcswif.py --mode replay --run file heep_hms_runlist_hcswif.dat --replay /u/group/E12-10-003/cyero/hallc_replay/UTIL_COMM_ONEPASS/SCRIPTS/HMS/replay_hms.C --disk 1000000000 --spectrometer HMS_ALL --project c-comm2017 --name heep_hms_scaler_replay           
python3 -m json.tool heep_hms_scaler_replay.json                                                                                                                                                                            
swif import -file  heep_hms_scaler_replay.json                                                                                                                                                                            
swif run heep_hms_scaler_replay  

